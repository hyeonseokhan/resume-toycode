{
  "compilerOptions": {
    // "incremental": true,
    // "baseUrl": ".",
    // "paths": {
    //   "@/*": ["./*"]
    // },
    "allowJs": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "outDir": "docs",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "out": "docs/typedoc",
    "mode": "file",
    "name": "Resume Next.js: TypeDoc",
    "externalPattern": "!**/I*.ts"
  }
}
